  "scripts": {
    "start": "electron-webpack dev",
    "build": "electron-webpack",
    "dist": "yarn run build && cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true electron-builder",
    "pack": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "pretest": "yarn run lint",
    "test": "NODE_ENV=test webpack --config config/webpack.renderer.babel.js && jest --env=jsdom --testMatch \"**/__tests__/**/*.test.{js,jsx,mjs}\" --setupTestFrameworkScriptFile \"./__tests__/setupTests.js\"",
    "lint": "eslint --env browser,node,server --ext .jsx,.js --color .",
    "lint:fix": "yarn run lint --fix",
    "clean": "rimraf build dist",
    "postinstall": "electron-builder install-app-deps && electron-rebuild --force"
  },
  "devDependencies": {
    "autoprefixer": "8.6.0",
    "babel-eslint": "8.2.2",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-classes": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.11",
    "electron": "2.0.0",
    "electron-builder": "20.11.1",
    "electron-devtools-installer": "2.2.4",
    "electron-rebuild": "1.7.3",
    "electron-webpack": "^2.1.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.4.0",
    "node-loader": "0.6.0",
    "node-sass": "^4.9.0",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.5",
    "raf": "3.4.0",
    "react-hot-loader": "^4.2.0",
    "react-router-test-context": "0.1.0",
    "redux-devtools-extension": "2.13.2",
    "redux-mock-store": "1.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "^7.0.2",
    "style-loader": "0.21.0",
    "svg-react": "^1.0.9",
    "thread-loader": "1.1.5",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-merge": "4.1.2",
    "webpack-node-externals": "1.7.2"
  },
  "build": {
    "appId": "io.nos.desktop",
    "win": {
      "icon": "./public/icons/icon1024x1024.ico"
    },
    "directories": {
      "buildResources": "public"
    },
    "extends": null
  },
  "electronWebpack": {
    "title": "nOS",
    "renderer": {
      "webpackConfig": "config/webpack.renderer.additions.js"
    }
  },
  "homepage": "./",
  "greenkeeper": {
    "ignore": [
      "webpack",
      "redux"
    ]
  }
}
